    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="n1, n2, n3, soma, m&#233;dia" type="Real" array="False" size=""/>
            <output expression="&quot;informe a primeira nota&quot;" newline="True"/>
            <input variable="n1"/>
            <if expression="n1&gt;10">
                <then>
                    <output expression="&quot;nota invalida&quot;" newline="True"/>
                </then>
                <else>
                    <output expression="&quot;informe a segunda nota&quot;" newline="True"/>
                    <input variable="n2"/>
                    <if expression="n2&gt;10">
                        <then>
                            <output expression="&quot;nota invalida&quot;" newline="True"/>
                        </then>
                        <else>
                            <output expression="&quot;informe a terceira nota&quot;" newline="True"/>
                            <input variable="n3"/>
