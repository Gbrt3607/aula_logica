                                <then>
                                    <output expression="&quot;nota invalida&quot;" newline="True"/>
                                </then>
                                <else>
                                    <assign variable="soma" expression="n1+n2+n3"/>
                                    <output expression="&quot; a soma das notas &#233;: &quot;&amp;soma" newline="True"/>
                                    <assign variable="m&#233;dia" expression="soma/3"/>
                                    <output expression="&quot; a m&#233;dia final &#233;: &quot;&amp;m&#233;dia" newline="True"/>
                                </else>
                            </if>
                        </else>
                    </if>
                </else>
            </if>
